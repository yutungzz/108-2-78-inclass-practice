if(!require("readr")){
  install.packages("readr")
}
sink("report.txt")
Sys.getenv()
Sys.getlocale()
readr::locale()
sessionInfo()
sink()
warning(
  'Please upload the following file: \n',
  file.path(getwd(),"report.txt"),
  '\nto the following website:\n',
  'https://forms.gle/vPoQ1MC4cNEJuNqM8'
)
